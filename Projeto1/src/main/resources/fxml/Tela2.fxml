<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane id="AnchorPane" prefHeight="97.0" prefWidth="404.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="br.edu.unirg.projeto1.Tela2Controller">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Digite um nome" />
      <TextField fx:id="textNome" layoutX="14.0" layoutY="30.0" prefHeight="26.0" prefWidth="376.0" />
      <HBox alignment="CENTER" layoutX="216.0" layoutY="60.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" onAction="#confirmarNome" text="Confirmar" />
            <Button mnemonicParsing="false" onAction="#cancelarSair" text="Cancelar" />
         </children>
      </HBox>
   </children>
</AnchorPane>
